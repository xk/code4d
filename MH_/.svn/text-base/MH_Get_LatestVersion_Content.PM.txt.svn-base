  //@use:  // $MethodContent_t := MH_Get_LatestVersion_Content ($MethodName_t:txt )If (False)  //=================  //@FUNCTION: MH_Get_LatestVersion_Content  //@brief: Returns the content of a method for the latest changelist  //=================  //@params:  // $1 (_t): MethodName  //@returns:  // $0 (_t) : Method content.  //=================  //@author: jpupier  //---------  //@version: 1.0  //@date: 21 may, 2010  //=================  //@SVN_Folder: MH_/Else   //=================C_TEXT($1;$MethodName_t)C_TEXT($0;$MethodContent_t)$MethodContent_t:=""  //---------C_LONGINT($ParamCount_l)$ParamCount_l:=Count parametersC_TEXT($External_DB_SQLPath_t)  //  //=================End if   //=================If ($ParamCount_l>=1)$MethodName_t:=$1$External_DB_SQLPath_t:=MH_Get_HistoPath Begin SQLUSE DATABASE DATAFILE :$External_DB_SQLPath_t;SELECT MethodContent FROM METHOD_HISTORY WHERE MethodName = :$MethodName_t ORDER BY ChangeList DESCLIMIT 1INTO :$MethodContent_t;USE LOCAL DATABASE DEFAULT;End SQLEnd if   //=================$0:=$MethodContent_t  //eof