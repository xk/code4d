  //MH_ParseImportRoot  //@SVN_Folder: MH_/C_TEXT($1;$Root_t)$Root_t:=$1C_POINTER($2;$FullFilePaths_at_p)$FullFilePaths_at_p:=$2C_POINTER($3;$MH_Import_Select_ab_p)$MH_Import_Select_ab_p:=$3C_POINTER($4;$MH_Import_SubDir_at_p)$MH_Import_SubDir_at_p:=$4C_POINTER($5;$MH_Import_FName_at_p)$MH_Import_FName_at_p:=$5C_POINTER($6;$MH_FileType_at_p)$MH_FileType_at_p:=$6C_POINTER($7;$MH_MethodType_at_p)$MH_MethodType_at_p:=$7C_POINTER($8;$MH_ModDate_ad_p)$MH_ModDate_ad_p:=$8C_POINTER($9;$MH_ModTime_at_p)$MH_ModTime_at_p:=$9C_POINTER($9;$MH_Import_FPath_at_p)$MH_Import_FPath_at_p:=$10C_LONGINT($rowcount_l;$loop_l)C_TEXT($folder_t;$subf_t;$doctype_t;$methodType_t;$CurrentPath_t;$methodName_t;$FileName_t)C_BOOLEAN($select_b)C_BOOLEAN($locked_b;$invisible_b)C_DATE($date_created_b;$date_modified_b)C_TIME($time_created_h;$time_modified_h)$rowcount_l:=Size of array($FullFilePaths_at_p->)ARRAY BOOLEAN($MH_Import_Select_ab_p->;$rowcount_l)ARRAY TEXT($MH_Import_SubDir_at_p->;$rowcount_l)ARRAY TEXT($MH_Import_FName_at_p->;$rowcount_l)ARRAY TEXT($MH_FileType_at_p->;$rowcount_l)ARRAY TEXT($MH_MethodType_at_p->;$rowcount_l)ARRAY DATE($MH_ModDate_ad_p->;$rowcount_l)ARRAY TEXT($MH_ModTime_at_p->;$rowcount_l)ARRAY TEXT($MH_Import_FPath_at_p->;$rowcount_l)  // root:subfolder:myfile.pm.txtFor ($loop_l;1;$rowcount_l)$CurrentPath_t:=$FullFilePaths_at_p->{$loop_l}$folder_t:=FILE_Get_Path_Parent ($CurrentPath_t)$FileName_t:=Replace string($CurrentPath_t;$folder_t;"")$subf_t:=Replace string($folder_t;$Root_t;"")$methodName_t:=$FileName_t$doctype_t:=FILE_Get_Extension ($methodName_t)$methodName_t:=FILE_Remove_Extension ($methodName_t)$methodType_t:=FILE_Get_Extension ($methodName_t)$methodName_t:=FILE_Remove_Extension ($methodName_t)GET DOCUMENT PROPERTIES($CurrentPath_t;$locked_b;$invisible_b;$date_created_b;$time_created_h;$date_modified_b;$time_modified_h)  //$select_b:=(($doctype_t="txt") | ($doctype_t="TEXT")) & (($methodType_t="PM")