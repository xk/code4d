C_TEXT($1;$methodbase)C_TEXT($out;$error)ASSERT(Count parameters>0;"One parameter is required")$methodbase:=$1Case of : ($methodbase="open")  // 4d opens. Start our own phpPHP_Gest ("start";->$out;->$error)If ($error#"")  // Component can't start phpALERT("The component can't start php. : \n"+$error)End if : ($methodbase="close")  // 4d closes. Stop our own php processesPHP_Gest ("stop";->$out;->$error)If ($error#"")  // Component can't start phpALERT("The component can't stop php. You have to stop them manualy (php-fcgi proc) : \n"+$error)End if Else   // errorEnd case 