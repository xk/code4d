  //@ CAll   //   $err := SVN_Revert ("myname";"mypwd";"/Path/to/my/working/local/repo/[file | Folder] ")  //  // ----------------------------------------------------  // Nom utilisateur (OS) : Pascal Foubert  // Date et heure : 20/11/10, 00:03:10  // ----------------------------------------------------  // Méthode : SVN_Revert  // Description  //  call svn revert  //    // Paramètres  //  IN ---------------------  //    C_TEXT($1) Username  //    C_TEXT($2) Password  //    C_TEXT($3) Full path of the local repo copy file or folder to add in the local repo (will then be added in versionning at next commit  //  OUT ---------------------  //    C_TEXT($0) Returned "1" if OK, err msg ifnot  // ----------------------------------------------------C_TEXT($1;$user;$2;$pwd;$3;$local)C_TEXT($0)ASSERT(Count parameters>0;"User parameter is missing")ASSERT(Count parameters>1;"Password parameter is missing")ASSERT(Count parameters>2;"Local working repo path parameter is missing")$user:=$1$pwd:=$2$local:=$3$php_script_path_t:=Get 4D folder(Current Resources folder)+"PHP"+Folder separator+"svn_revert.php"SET ENVIRONMENT VARIABLE("SVN_LOCAL";$local)SET ENVIRONMENT VARIABLE("SVN_USER";$user)SET ENVIRONMENT VARIABLE("SVN_PASSWORD";$pwd)C_TEXT($result_t)PHP Execute($php_script_path_t;"";$result_t)If (Substring($result_t;1;1)="1")$0:="1"Else $0:=$result_tEnd if 