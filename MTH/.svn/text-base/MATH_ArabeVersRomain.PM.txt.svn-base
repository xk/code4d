If (False)  //MATH_ArabeVersRomain   //Auteur : Olivier Deschanels   //Date : 27/02/2003  // Elément du projet Vitruve © 1991  // ------------------------------------------------------  // ---------------   F R A N C A I S  ---------------  // ------------------------------------------------------  // Conversion d'un entier en chaine de chiffre romain.  // Note : les entiers suffisent car les romains ne traitaient pas de grands nombre  // MATH_ArabeVersRomain (Entier) -> TexteRésultat  // MATHERROR  // 0 -> Pas d'erreur  // ------------------------------------------------------  // ---------------   E N G L I S H   -----------------  // ------------------------------------------------------  // This method converts a integer to a string with roman numeral  // Notice : integers are sufficient, because Roman numeral does not compute big numbers  // MATH_ArabeVersRomain (Integer) -> Text  // MATHERROR  // 0 -> No error  // ---------------------------------------------  //  (translation : BE 03-2010)End if   //@SVN_Folder: MATH_/C_TEXT($0;$resultat)C_LONGINT($1;$chiffre)C_BOOLEAN($fin)C_TEXT($d;$c;$u)C_TEXT($aj)  //initialisation$fin:=False$chiffre:=$1$resultat:=""MATHERROR:=0  //calculRepeat $num:=$chiffreCase of : ($num>999)$aj:="M"*Num(Substring(String($chiffre);1;1)): ($num>99)$u:="C"$c:="D"$d:="M": ($num>9)$u:="X"$c:="L"$d:="C"Else $u:="I"$c:="V"$d:="X"End case If ($num<1000)$num:=Num(Substring(String($chiffre);1;1))$aj:=""Case of : ($num<4)$aj:=$u*$num: ($num=4)$aj:=$u+$c: ($num=5)$aj:=$c: ($num<9)$aj:=$c+($u*($num-5)): ($num=9)$aj:=$u+$dEnd case End if $resultat:=$resultat+$aj$chiffre:=Num(Substring(String($chiffre);2))$fin:=($chiffre<=0)Until ($fin=True)$0:=$resultat