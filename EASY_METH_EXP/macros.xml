		</macro>

		<macro name="A0_OnSave" method_event="on_save" version="2" type_ahead="false" in_menu="false">
		<text><method>A0_MACRO_CALL_BACK("on_save";"<method_name/>";"<user_os/>")</method></text>		<!-- Called whenever a method is saved-->
		</macro>

		<macro name="A0_OnClose" method_event="on_close" version="2" type_ahead="false" in_menu="false">
		<text><method>A0_MACRO_CALL_BACK("on_close";"<method_name/>";"<user_os/>")</method></text>		<!-- Called whenever a method is closed-->
		</macro>
</macros>

