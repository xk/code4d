<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE macros SYSTEM "http://www.4d.com/dtd/2007/macros.dtd" >
<macros>		
		<macro name="A0_OnCreate" method_event="on_create" version="2" type_ahead="false" in_menu="false">
		<text><method>A0_MACRO_CALL_BACK("on_create";"<method_name/>";"<user_os/>")</method></text>		<!-- Called whenever a method is created-->
		</macro>

		<macro name="A0_OnLoad" method_event="on_load" version="2" type_ahead="false" in_menu="false">
